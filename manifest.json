{
  "manifest_version": 2,

  "name": "Switchify",
  "description": "An extension to improve your productivity and have a clutter free browsing session",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "linux": "Ctrl+Shift+0"
      }
    }
  },

  "permissions": ["activeTab", "tabs"]
}